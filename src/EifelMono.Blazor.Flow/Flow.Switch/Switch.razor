@namespace EifelMono.Blazor.Flow
@typeparam T

<CascadingValue Value="@this">
    @ChildContent
</CascadingValue>

@code {
    [Parameter]
    public T Value { get; set; }
    [Parameter]
    public RenderFragment ChildContent { get; set; }

    public bool IsEquals(object value)
        => object.Equals(Value, value);
}
